{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "priceRangeMin": {
      "type": "integer",
      "required": true
    },
    "priceRangeMax": {
      "type": "integer",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "isActive": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "area": {
      "type": "enumeration",
      "enum": [
        "Cairo",
        "Giza",
        "Alexandria",
        "Sharm el Sheikh",
        "Luxor",
        "Aswan",
        "Hurghada",
        "Gouna"
      ],
      "required": true
    },
    "subcategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subcategory.subcategory",
      "inversedBy": "services"
    },
    "webLink": {
      "type": "string"
    },
    "operatingHoursStart": {
      "type": "time",
      "required": false
    },
    "operatingHoursEnd": {
      "type": "time"
    },
    "provider": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::provider.provider"
    },
    "location": {
      "type": "string"
    },
    "isFeatured": {
      "type": "boolean"
    },
    "cover": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
